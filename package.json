    "react",
    "slots"
  ],
  "devDependencies": {
    "@arkweid/lefthook": "0.7",
    "@testing-library/react": "12",
    "@testing-library/jest-dom": "5",
    "@types/jest": "27",
    "@types/react": "17",
    "@types/react-dom": "17",
    "doctoc": "2",
    "effector": "22",
    "effector-react": "22",
    "jest": "27",
    "microbundle": "0.14.1",
    "prettier": "2",
    "react": "17",
    "react-dom": "17",
    "ts-jest": "27",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "effector": ">= 21.8.0",
    "effector-react": ">= 21.3.0",
    "react": ">= 16.8"
  }
